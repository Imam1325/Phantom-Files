# AWS Access Credentials
# Generated: {{ date }}
# Owner: {{ admin_name }} <{{ admin_email }}>

[default]
aws_access_key_id = AKIA{{ aws_key }}
aws_secret_access_key = {{ db_password }}{{ sentry_key[:10] }}
region = us-east-1
output = json

[backup-user]
aws_access_key_id = AKIA{{ sentry_key[:16].upper() }}
aws_secret_access_key = {{ db_password }}_backup
region = eu-west-1